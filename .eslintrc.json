{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/core-web-vitals"],
  "ignorePatterns": ["node_modules/"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
